<h1>My Bookings</h1>

<%= link_to "Dashboard", pages_path %>
<%= link_to "My Dogs", pages_my_dogs_path %>
<%= link_to "My Bookings", pages_my_bookings_path %>
<%= link_to "Account", pages_my_account_path %>

<ul>
  <% current_user.bookings.each do |booking| %>
    <li>
      <%= booking.dog.name %> on <%= booking.date %> from <%= booking.dog.user.first_name %> <%= link_to "View this booking", dog_booking_path(booking.dog, booking) %>
    </li>
  <% end %>
</ul>
